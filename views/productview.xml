<odoo>
  <data>
  <!-- Product kanban -->
<record model="ir.ui.view" id="productss_list">
      <field name="name">Products list</field>
      <field name="model">productss.productss</field>
      <field name="arch" type="xml">
        <kanban>
          <field name="name"/>
        </kanban>
      </field>
    </record>
  <!-- Graph -->
    <record model="ir.ui.view" id="product_graph">
    <field name="name">productss.productss.graph</field>
    <field name="model">productss.productss</field>
    <field name="arch" type="xml">
        <graph string="Quotation" type="bar">
            <field name="name" group="True"/>
            <field name="price" type="measure"/>
            <field name="create_date" interval="day" />
        </graph>
    </field>
    </record>

    <!-- Product list  -->
    
    <record model="ir.ui.view" id="productss_list">
      <field name="name">Products list</field>
      <field name="model">productss.productss</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="description"/>
          <field name="price"/>
          <field name="qte"/>
        </tree>
      </field>
    </record>
<!--  Client list -->
    <record model="ir.ui.view" id="clientt_list">
      <field name="name">Client list</field>
      <field name="model">clientt.clientt</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name_client"/>
          <field name="email_client"/>
          <field name="adresse_client"/>
          <field name="numerotel_client"/>
        </tree>
      </field>
    </record>
<!-- Fourni list -->
    <record model="ir.ui.view" id="fournisseurr_list">
      <field name="name">Fourni list</field>
      <field name="model">fournisseurr.fournisseurr</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="email_fourni"/>
          <field name="adresse_fourni"/>
          <field name="numerotel_fourni"/>
        </tree>
      </field>
     
    </record>

    <!-- Inventaire list -->
    <record model="ir.ui.view" id="inventairee_list">
      <field name="name">Inventaire list</field>
      <field name="model">inventairee.inventairee</field>
      <field name="arch" type="xml">
        <tree>
          <field name="date"/>
          <field name="state"/>
        </tree>
      </field>
    </record>

    <!-- Commande list -->
    <record model="ir.ui.view" id="commandee_list">
      <field name="name">Commande list</field>
      <field name="model">commandee.commandee</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="totalcmd"/>
        </tree>
      </field>
    </record>

   <!--   Achat form -->
    <record model="ir.ui.view" id="commandee_form">
      <field name="name">Achat form</field>
      <field name="model">commandee.commandee</field>
      <field name="arch" type="xml">
        <form string="cmdForm" edit="false">
            <header>
                <button type="object" name="achatfunc" string="Validate Achat" clas="oe_highlight" states="draft"/>
                <field name="state" widget="statusbar" statusbar_visible="draft,confirm"/>
            </header> 
            <sheet>
                <group>
                  <field name="id_fournisseur"/>
                </group>
                <group>
                  <field name="id_cmdqte"/>
                </group>
            </sheet>
        </form>
      </field>
    </record> 


    <!--   Inventaire form -->
    <record model="ir.ui.view" id="inventairee_form">
      <field name="name">Inventaire form</field>
      <field name="model">inventairee.inventairee</field>
      <field name="arch" type="xml">
        <form string="inventaireForm">
            <header>
                <button type="object" name="changeqte" string="Validate Inventory" clas="oe_highlight" states="draft"/>
                <field name="state" widget="statusbar" statusbar_visible="draft,confirm"/>
            </header>
            <sheet>
                <group>
                  <field name="date" readonly="1"/>
                </group>
                <group>
                  <field name="id_inv"/>
                </group>
            </sheet>
        </form>
      </field>
    </record> 

<!-- PdtInventory list -->
    <record model="ir.ui.view" id="PdtInventory_list">
      <field name="name">PdtInventory list</field>
      <field name="model">pdtinventory.pdtinventory</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="id_product"/>
          <field name="theoricalqte"/>
          <field name="realqte"/>
        </tree>
      </field>
    </record>

    

    <!-- Charge list -->
    <record model="ir.ui.view" id="chargee_list">
      <field name="name">Charge list</field>
      <field name="model">chargee.chargee</field>
      <field name="arch" type="xml">
        <tree>
          <field name="nom"/>
          <field name="description"/>
          <field name="prix"/>
        </tree>
      </field>
    </record>

  <!-- Commande list -->
    <record model="ir.ui.view" id="achat_list">
      <field name="name">Achat list</field>
      <field name="model">commandee.commandee</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="state"/>
        </tree>
      </field>
    </record>

    <!-- cmdpdct list -->
    <record model="ir.ui.view" id="chargee_list">
      <field name="name">Commande list</field>
      <field name="model">cmdqte.cmdqte</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="id_product"/>
          <field name="qte"/>
          <field name="price_product"/>
          <field name="total" sum='totalcmd'/>
        </tree>
      </field>
    </record>
  
  <!-- Test -->
    <record model="ir.actions.act_window" id="productss_action_window">
      <field name="name">List Window</field>
      <field name="res_model">mods.mods</field>
      <field name="view_mode">tree,form</field>
      
    </record>

    <!-- Action Products -->
    <record model="ir.actions.act_window" id="productss_action2_window">
      <field name="name">Products Window</field>
      <field name="res_model">productss.productss</field>
      <field name="view_mode">tree,form,graph,kanban</field>
    </record>


    <!-- Action Client -->
    <record model="ir.actions.act_window" id="productss_action4_window">
      <field name="name">Client Window</field>
      <field name="res_model">clientt.clientt</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- Action Fourni -->
    <record model="ir.actions.act_window" id="productss_action5_window">
      <field name="name">Fournisseur Window</field>
      <field name="res_model">fournisseurr.fournisseurr</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- Action Inventaire -->
    <record model="ir.actions.act_window" id="productss_action6_window">
      <field name="name">Inventaire Window</field>
      <field name="res_model">inventairee.inventairee</field>
      <field name="view_mode">tree,form</field>
    </record>

    


    <!-- Action Charge -->
    <record model="ir.actions.act_window" id="productss_action8_window">
      <field name="name">Charge Window</field>
      <field name="res_model">chargee.chargee</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- Action Commande -->
    <record model="ir.actions.act_window" id="productss_action9_window">
      <field name="name">Achat Window</field>
      <field name="res_model">commandee.commandee</field>
      <field name="view_mode">tree,form</field>
    </record>

    
    <!-- Menu items parent -->
    <menuitem name="productss" id="productss_menu_root"/>
    
    <menuitem name="Menu" id="productss_menu_1" parent="productss_menu_root"/>
    
    <!-- Menu items Child Childs -->
    <menuitem name="List" id="productss_menu_2_list" parent="productss_menu_1"
              action="productss_action_window"/>
    
    <!-- Menu items Childs -->
       
    <menuitem name="Products" id="productss_menu_2" parent="productss_menu_root"
              action="productss_action2_window"/>

    <menuitem name="Client" id="productss_menu_4" parent="productss_menu_root"
              action="productss_action4_window"/>
    <menuitem name="Fournisseur" id="productss_menu_5" parent="productss_menu_root"
              action="productss_action5_window"/>
    <menuitem name="Inventaire" id="productss_menu_6" parent="productss_menu_root"
              action="productss_action6_window"/>         

    <menuitem name="Charge" id="productss_menu_8" parent="productss_menu_root"
              action="productss_action8_window"/> 
    <menuitem name="Achat" id="productss_menu_9" parent="productss_menu_root"
              action="productss_action9_window"/> 


  </data>
</odoo>