
<odoo>

<template id="report_achat">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="o">
            <t t-call="web.external_layout">
                <div class="page">
                    <h2>Report achat</h2>
                    <p>achat number  is <span t-field="o.name"/></p>

                    <table style="width:100%">
                    <tr>
                        <th>fournissuer</th>
                        
                        <th>total</th>
                    </tr>
                    <tr>
                        <td> <span t-field="o.id_fournisseur"/></td>
                        <td><span t-field="o.totalcmd"/> DH</td> 
                        
                    </tr>
                   
                    </table>

                </div>
            </t>
        </t>
    </t>
</template>

</odoo>
