<odoo>
  <data>
    <!-- explicit list view definition -->
    
    <record model="ir.ui.view" id="mods.list">
      <field name="name">mods list</field>
      <field name="model">mods.mods</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>
   

    <!-- actions opening views on models -->
  
    <record model="ir.actions.act_window" id="mods.action_window">
      <field name="name">mods window</field>
      <field name="res_model">mods.mods</field>
      <field name="view_mode">tree,form</field>
    </record>
    

    <!-- server action to the one above -->
    
    <!-- <record model="ir.actions.server" id="mods.action_server">
      <field name="name">mods server</field>
      <field name="model_id" ref="model_mods_mods"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>
     -->

    <!-- Top menu item -->
    
    <menuitem name="mods" id="mods.menu_root"/>
    
    <!-- menu categories -->
    
    <menuitem name="Menu 1" id="mods.menu_1" parent="mods.menu_root"/>
    <!--
    <menuitem name="Menu 2" id="mods.menu_2" parent="mods.menu_root"/>
    -->
    <!-- actions -->
    
    <menuitem name="List" id="mods.menu_1_list" parent="mods.menu_1"
              action="mods.action_window"/>
              <!--
    <menuitem name="Server to list" id="mods" parent="mods.menu_2"
              action="mods.action_server"/>
    -->
  </data>
</odoo>